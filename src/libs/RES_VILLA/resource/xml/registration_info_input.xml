<page background="./image/villa_bg.png">
  <ctrlgrp lbmask="l">
     <button valkey="FIRSTNAME"
	     focused_bg="./image/browser_list_btn.png"/> 
     <linebreak/>
     <button valkey="LASTNAME"
	     focused_bg="./image/browser_list_btn.png"/> 
     <linebreak/>
     <button valkey="EMAIL"
	     focused_bg="./image/browser_list_btn.png"/> 
     <linebreak/>
     <button valkey="OK"
	      focused_bg="image/Villa_button_blue_f_02.png"
	      normal_bg="image/Villa_button_blue_n_02.png"/>
     <button valkey="CANCEL"
	      focused_bg="image/Villa_button_blue_f_02.png"
	      normal_bg="image/Villa_button_blue_n_02.png"/>
  </ctrlgrp>
  <text text="$$Register Product" x="249" y="132" w="600" h="44" align="left" fontsize="42" textcolor="0x07b5ff"/>

  <table x="281" y="228" w="719" h="153"
	             cols="1" rows="3" colspacing="0" rowspacing="0" >
	<image image="@@bg-FIRSTNAME"/>
	<image image="@@bg-LASTNAME"/>
	<image image="@@bg-EMAIL"/>
  </table>

  <table x="281" y="228" w="719" h="255"
         cols="1" rows="5" colspacing="0" rowspacing="0" align="left" xmargin="20" fontsize="26" textcolor="0xe2e2e2">
	<text text="$$First Name"/>
	<text text="$$Last Name"/>
	<text text="$$Email"/>
	<text text="$$Serial Number" textcolor="0x929292"/>
	<text text="$$Mac Address" textcolor="0x929292"/>
  </table>

  <wrapper x="293" w="695" h="1">
	<image image="./image/browser_list_line.png" y="277"/>
	<image image="./image/browser_list_line.png" y="328"/>
	<image image="./image/browser_list_line.png" y="379"/>
	<image image="./image/browser_list_line.png" y="430"/>
	<image image="./image/browser_list_line.png" y="481"/>
  </wrapper>
  
  <imgbtn image="@@bg-OK" text="$$Submit" textcolor="0xe2e2e2" x="281" y="505" w="343" h="78" fontsize="18" align="hcenter"/>
  <imgbtn image="@@bg-CANCEL" text="$$Cancel" textcolor="0xe2e2e2" x="655" y="505" w="343" h="78" fontsize="18" align="hcenter"/>

  <wrapper x="630" w="330" h="51" align="right" delay="2">
  	<text text="@@first_name_value" y="228" fontsize="26" textcolor="0xe2e2e2" speed="@@sel-FIRSTNAME"/>
  	<text text="@@last_name_value" y="279" fontsize="26" textcolor="0xe2e2e2" speed="@@sel-LASTNAME"/>
  	<text text="@@email_value" y="330" fontsize="26" textcolor="0xe2e2e2" speed="@@sel-EMAIL"/>
  	<text text="@@serial_number_value" y="381" fontsize="26" textcolor="0x929292" speed="@@sel-OK"/>
  	<text text="@@mac_address_value" y="432" fontsize="26" textcolor="0x929292" speed="@@sel-CANCEL"/>
  </wrapper>
</page>
