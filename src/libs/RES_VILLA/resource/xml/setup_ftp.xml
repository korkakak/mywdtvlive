<page background="image/setup.jpg">
  <ctrlgrp lbmask="lu">
    <string valkey="FTPPATH" maxlen="128"
             value="value-FTPPATH" title="$$: FTP PATH"
             focused_bg="image/textbox_highlight.gif"
             normal_bg="image/textbox.gif"/>
    <linebreak />
    <button valkey="OK"
             focused_bg="image/button_blank-act.png"
             normal_bg="image/button_blank.png"/>
    <button valkey="CANCEL"
             focused_bg="image/button_blank-act.png"
             normal_bg="image/button_blank.png"/>
  </ctrlgrp>
  
  <text text="$$FTP PATH:" x="100" y="200" w="147" h="28" fontsize="20" align="right"/>
  
  <!-- FTP PATH -->
  <image image="@@bg-FTPPATH" 
         x="250" y="200" w="219" h="25"/>
  <text text="@@FTPPATH" active="@@act-FTPPATH" 
        x="250" y="193" w="219" h="40" 
        textcolor="0x333333" activetextcolor="0xC0C000"  actnoseltextcolor="0xC0C000" fontsize="25"/>
  
  <table x="250" y="250" w="200" h="67" cols="2" rows="1" rowspacing="0" align="hcenter" fontsize="18">
    <imgbtn image="@@bg-OK" text="$$OK"/>
    <imgbtn image="@@bg-CANCEL" text="$$Cancel"/>
  </table>
  
  <!-- fixed -->
  <include filename="./inc_nav.xml"/>
  <include filename="./info.xml"/>  
  <include filename="./inc_headline_property_ftp.xml"/>
  <include filename="./decoration.xml"/>
</page>
