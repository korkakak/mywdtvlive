	<wrapper x="0" w="307" h="1">
    	<image image="@@options_0_line" ymargin="0" y="194"/>
	    <image image="@@options_1_line" ymargin="0" y="243"/>
    	<image image="@@options_2_line" ymargin="0" y="292"/>
	    <image image="@@options_3_line" ymargin="0" y="341"/>
	    <image image="@@options_4_line" ymargin="0" y="390"/>
	    <image image="@@options_5_line" ymargin="0" y="439"/>
	</wrapper>

  <include filename="./inc_youtube_logo.xml"/>
</page>
