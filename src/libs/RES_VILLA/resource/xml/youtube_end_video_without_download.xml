<?xml version="1.0"?>
<!--<page type="basic" overlay="1">-->
<page background="image/youtube_cover_up.png">
	<ctrlgrp lbmask="LR">
    	<listbox valkey="browselist" autoenter="1" move="page" cols="1" rows="5" lbmask="lr"  loop_scroll="1" disable_light_bar_click="1"/>
		<listbox valkey="end_video_options" move="row" autoenter="1" cols="1" rows="6" lbmask="lr" disable_light_bar_click="1"/>
	</ctrlgrp>

	<table x="378" y="138" w="558" h="495" cols="1" rows="5">
		<image image="image/youtube/youtube_end_video_Thumbnails_Select_frame_n.png" w="558" h="99"/>
		<image image="image/youtube/youtube_end_video_Thumbnails_Select_frame_n.png" w="558" h="99"/>
		<image image="image/youtube/youtube_end_video_Thumbnails_Select_frame_n.png" w="558" h="99"/>
		<image image="image/youtube/youtube_end_video_Thumbnails_Select_frame_n.png" w="558" h="99"/>
		<image image="image/youtube/youtube_end_video_Thumbnails_Select_frame_n.png" w="558" h="99"/>
	</table>
  
	<table x="378" y="138" w="558" h="495" fontsize="14" selected_fontsize="14"
         cols="1" rows="5" colspacing="0" rowspacing="0"
		 lbimage="image/youtube/youtube_end_video_Thumbnails_Select_frame_f.png" clickimage=""  
		 textcolor="0xe2e2e2" actnoseltextcolor="0xe2e2e2" actoverlistcolor="0xe2e2e2" 
		 activetextcolor="0xe2e2e2" selectedtextcolor="0xe2e2e2">
    	<text text="@@browse_0_author" lmargin="143" ymargin="43"
		  lightbar="@@lightbar-browselist-0" active="@@act-browselist"
		  selected="@@browse_0_select"/>
	    <text text="@@browse_1_author" lmargin="143" ymargin="43"
		  lightbar="@@lightbar-browselist-1" active="@@act-browselist" 
		  selected="@@browse_1_select"/>
    	<text text="@@browse_2_author" lmargin="143" ymargin="43"
		  lightbar="@@lightbar-browselist-2" active="@@act-browselist" 
		  selected="@@browse_2_select"/>
	    <text text="@@browse_3_author" lmargin="143" ymargin="43"
		  lightbar="@@lightbar-browselist-3" active="@@act-browselist" 
		  selected="@@browse_3_select"/>
    	<text text="@@browse_4_author" lmargin="143" ymargin="43"
		  lightbar="@@lightbar-browselist-4" active="@@act-browselist" 
		  selected="@@browse_4_select"/>
	</table>
 
	<!-- Stars -->
	<wrapper x="801" w="100" h="19">
		<image image="@@browse_0_stars" ymargin="0" y="200"/>
		<image image="@@browse_1_stars" ymargin="0" y="299"/>
		<image image="@@browse_2_stars" ymargin="0" y="398"/>
		<image image="@@browse_3_stars" ymargin="0" y="497"/>
		<image image="@@browse_4_stars" ymargin="0" y="596"/>
	</wrapper>

	<!-- Video title -->
	<wrapper x="521" w="388" h="22" fontsize="20" textcolor="0xffffff">
		<text text="@@browselist_0_text" y="151"  speed="@@browse_0_speed" delay="2"/>
		<text text="@@browselist_1_text" y="250"  speed="@@browse_1_speed" delay="2"/>
		<text text="@@browselist_2_text" y="349"  speed="@@browse_2_speed" delay="2"/>
		<text text="@@browselist_3_text" y="448"  speed="@@browse_3_speed" delay="2"/>
		<text text="@@browselist_4_text" y="547"  speed="@@browse_4_speed" delay="2"/>
	</wrapper>

	<!-- Video date -->
	<wrapper x="521" w="100" h="16" fontsize="14" textcolor="0xe2e2e2">
		<text text="@@browse_0_date" y="205"/>
		<text text="@@browse_1_date" y="304"/>
		<text text="@@browse_2_date" y="403"/>
		<text text="@@browse_3_date" y="502"/>
		<text text="@@browse_4_date" y="601"/>
	</wrapper>
	<!-- Video views -->
	<wrapper x="631" w="160" h="16" fontsize="14" textcolor="0xe2e2e2">
		<text text="@@browse_0_views" y="205"/>
		<text text="@@browse_1_views" y="304"/>
		<text text="@@browse_2_views" y="403"/>
		<text text="@@browse_3_views" y="502"/>
		<text text="@@browse_4_views" y="601"/>
	</wrapper>

<!--	<wrapper x="443" w="120" h="70" align="hcenter" scale_type="fixratio" scale="100">-->
	<wrapper x="388" w="126" h="82" align="hcenter" scale="100">
	    <image image="@@browse_0_icon" ymargin="0" y="147"/>
    	<image image="@@browse_1_icon" ymargin="0" y="246"/>
	    <image image="@@browse_2_icon" ymargin="0" y="345"/>
    	<image image="@@browse_3_icon" ymargin="0" y="444"/>
	    <image image="@@browse_4_icon" ymargin="0" y="543"/>
  	</wrapper>

	<text text="@@option_title" x="380" y="77" w="556" h="28" fontsize="26" textcolor="0x07b5ff"/>
	<image image="image/villa_arrow_up.png" x="633" y="109" w="40" h="29"/>
	<image image="image/villa_arrow_down.png" x="633" y="637" w="40" h="29" />

<!--
	<text text="@@options" x="90" y="598" w="500" h="52" align="left" fontsize="50" textcolor="0x07B5FF" />
-->

	<table x="0" y="146" w="307" h="294"
    		cols="1" rows="6" colspacing="0" rowspacing="0"
    		lbimage="image/youtube/youtube_submenu_select_bar.png"
    		fontsize="18" selected_fontsize="18"
   			textcolor="0xe2e2e2" align="left">
    	<text text="@@options_0_text" lmargin="95" ymargin="14" speed="@@options_0_speed" delay="2"
          	lightbar="@@lightbar-end_video_options-0" active="@@act-end_video_options"/>
    	<text text="@@options_1_text" lmargin="95" ymargin="14" speed="@@options_1_speed" delay="2"
          	lightbar="@@lightbar-end_video_options-1" active="@@act-end_video_options"/>
    	<text text="@@options_2_text" lmargin="95" ymargin="14" speed="@@options_2_speed" delay="2"
          	lightbar="@@lightbar-end_video_options-2" active="@@act-end_video_options"/>
    	<text text="@@options_3_text" lmargin="95" ymargin="14" speed="@@options_3_speed" delay="2"
          	lightbar="@@lightbar-end_video_options-3" active="@@act-end_video_options"/>
    	<text text="@@options_4_text" lmargin="95" ymargin="14" speed="@@options_4_speed" delay="2"
          	lightbar="@@lightbar-end_video_options-4" active="@@act-end_video_options"/>
