<?xml version="1.0"?>
<page>
	<ctrlgrp lbmask="LRud">
		<button valkey="ToHome" 
			normal_bg="image/blockbuster/bb_browse_home_n.png"
			focused_bg="image/blockbuster/bb_browse_home_f.png"/>
		<linebreak/>
		<button valkey="Running"
			normal_bg="image/blockbuster/bb_bt_n.png" 
			focused_bg="image/blockbuster/bb_bt_f.png" 
			disabled_bg="image/blockbuster/bb_bt_d.png"
			click_bg="image/blockbuster/bb_bt_c.png" click_effect="tp"/>
		<button valkey="Completed"
			normal_bg="image/blockbuster/bb_bt_n.png" 
			focused_bg="image/blockbuster/bb_bt_f.png" 
			disabled_bg="image/blockbuster/bb_bt_d.png"
			click_bg="image/blockbuster/bb_bt_c.png" click_effect="tp"/>
		<button valkey="Schedule"
			normal_bg="image/blockbuster/bb_bt_n.png"
			focused_bg="image/blockbuster/bb_bt_f.png"
			click_bg="image/blockbuster/bb_bt_c.png"
			clicked_bg="image/blockbuster/bb_bt_n.png" click_effect="tp"/>
		<button valkey="Notification"
			normal_bg="image/blockbuster/bb_bt_n.png"
			focused_bg="image/blockbuster/bb_bt_f.png"
			click_bg="image/blockbuster/bb_bt_c.png"
			clicked_bg="image/blockbuster/bb_bt_n.png" click_effect="tp"/>
		<button valkey="FileLocation"
			normal_bg="image/blockbuster/bb_bt_n.png"
			focused_bg="image/blockbuster/bb_bt_f.png"
			click_bg="image/blockbuster/bb_bt_c.png"
			clicked_bg="image/blockbuster/bb_bt_n.png" click_effect="tp"/>
		<linebreak/>
		<listbox valkey="serverlist" move="page" cols="1" rows="6" autoenter="1"
			focused_bg="image/blockbuster/bb_prefrernce_Select-bar_f.png"
			click_bg="image/blockbuster/bb_prefrernce_Select-bar_c.png"/>
	</ctrlgrp>

	<table x="105" y="140" w="1070" h="49" fontsize="20" cols="5" rows="1" align="hcenter" colspacing="5" textcolor="0xff727272" actoverlistcolor="0xff727272" activetextcolor="0xfff2f2f2" clicktextcolor="0xff4d4d4d" clickedtextcolor="0xfffcb040">
		<imgbtn image="@@bg-Running" text="Download" 
			active="@@act-Running" selected="@@sel-Running" click="@@click-Running" 
			clicked="@@clicked-Running"/>
		<imgbtn image="@@bg-Completed" text="Completed" 
			active="@@act-Completed" selected="@@sel-Completed" click="@@click-Completed" 
			clicked="@@clicked-Completed"/>
		<imgbtn image="@@bg-Schedule" text="Download Schedule" 
			active="@@act-Schedule" selected="@@sel-Schedule" click="@@click-Schedule" 
			clicked="@@clicked-Schedule"/>
		<imgbtn image="@@bg-Notification" text="Notification" 
			active="@@act-Notification" selected="@@sel-Notification" click="@@click-Notification" 
			clicked="@@clicked-Notification"/>
		<imgbtn image="@@bg-FileLocation" text="Download Location"
			active="@@act-FileLocation" selected="@@sel-FileLocation" click="@@click-FileLocation" 
			clicked="@@clicked-FileLocation"/>
	</table>

	<table x="316" y="224" w="648" h="306" cols="1" rows="6">
		<imgbtn image="@@bg-serverlist-0" active="@@act-serverlist-0" selected="@@sel-serverlist-0" click="@@click-serverlist-0" clicked="@@clicked-serverlist-0" disable="@@disable-serverlist"/>
		<imgbtn image="@@bg-serverlist-1" active="@@act-serverlist-1" selected="@@sel-serverlist-1" click="@@click-serverlist-1" clicked="@@clicked-serverlist-1" disable="@@disable-serverlist"/>
		<imgbtn image="@@bg-serverlist-2" active="@@act-serverlist-2" selected="@@sel-serverlist-2" click="@@click-serverlist-2" clicked="@@clicked-serverlist-2" disable="@@disable-serverlist"/>
		<imgbtn image="@@bg-serverlist-3" active="@@act-serverlist-3" selected="@@sel-serverlist-3" click="@@click-serverlist-3" clicked="@@clicked-serverlist-3" disable="@@disable-serverlist"/>
		<imgbtn image="@@bg-serverlist-4" active="@@act-serverlist-4" selected="@@sel-serverlist-4" click="@@click-serverlist-4" clicked="@@clicked-serverlist-4" disable="@@disable-serverlist"/>
		<imgbtn image="@@bg-serverlist-5" active="@@act-serverlist-5" selected="@@sel-serverlist-5" click="@@click-serverlist-5" clicked="@@clicked-serverlist-5" disable="@@disable-serverlist"/>
	</table>

	<table x="316" y="224" w="600" h="306" cols="1" rows="6" fontsize="20" align="left,top" textcolor="0xff727272" actoverlistcolor="0xff727272" activetextcolor="0xfff2f2f2" clicktextcolor="0xff4d4d4d" clickedtextcolor="0xfff54029">
		<text text="@@serverlist_0_text" active="@@act-serverlist-0" selected="@@sel-serverlist-0" click="@@click-serverlist-0" disable="@@disable-serverlist" lmargin="20" umargin="16"/>
		<text text="@@serverlist_1_text" active="@@act-serverlist-1" selected="@@sel-serverlist-1" click="@@click-serverlist-1" disable="@@disable-serverlist" lmargin="20" umargin="16"/>
		<text text="@@serverlist_2_text" active="@@act-serverlist-2" selected="@@sel-serverlist-2" click="@@click-serverlist-2" disable="@@disable-serverlist" lmargin="20" umargin="16"/>
		<text text="@@serverlist_3_text" active="@@act-serverlist-3" selected="@@sel-serverlist-3" click="@@click-serverlist-3" disable="@@disable-serverlist" lmargin="20" umargin="16"/>
		<text text="@@serverlist_4_text" active="@@act-serverlist-4" selected="@@sel-serverlist-4" click="@@click-serverlist-4" disable="@@disable-serverlist" lmargin="20" umargin="16"/>
		<text text="@@serverlist_5_text" active="@@act-serverlist-5" selected="@@sel-serverlist-5" click="@@click-serverlist-5" disable="@@disable-serverlist" lmargin="20" umargin="16"/>
	</table>

	<image image="@@status_icon" x="585" y="303" w="113" h="113"/>
	<include filename="./blockbuster_common.xml"/>
</page>
