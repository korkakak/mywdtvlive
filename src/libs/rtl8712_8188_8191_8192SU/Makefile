TOP        := $(shell pwd)/../../..#
include $(TOP)/Target.mk
include $(shell pwd)/rtl8712_8188_8191_8192SU_usb_linux_v2.6.0005.20091112/config

LINUX_SRC = $(TOP_LIBS_DIR)/$(CONF_SIGMA_SDK_VERSION)/linux_kernel_$(CONF_LINUX_KERNEL_VERSION)/
CROSS_COMPILE = $(CROSS)
CROSS_COMPILE_INCLUDE = $(CONF_CROSS_PREFIX)include
LINUX_SRC_MODULE = $(TOP)/build/root/lib/modules

export LINUX_SRC CROSS_COMPILE CROSS_COMPILE_INCLUDE LINUX_SRC_MODULE CONFIG_RTL8711 CONFIG_RTL8712 CONFIG_RTL_WLAN_11N CONFIG_RTL819XS CONFIG_USB_HCI CONFIG_SDIO_HCI CONFIG_MP_INCLUDED CONFIG_PLATFORM_I386_PC CONFIG_PLATFORM_ARM_S3C CONFIG_PLATFORM_ARM_PXA CONFIG_PLATFORM_MIPS_RMI CONFIG_PLATFORM_RTD2880B CONFIG_MLME_EXT CONFIG_DRVEXT_MODULE

all: 
	cp $(shell pwd)/rtl8712_8188_8191_8192SU_usb_linux_v2.6.0005.20091112/autoconf_rtl8712_usb_linux.h $(shell pwd)/rtl8712_8188_8191_8192SU_usb_linux_v2.6.0005.20091112/include/autoconf.h
	make  -C  $(LINUX_SRC) SUBDIRS=$(shell pwd)/rtl8712_8188_8191_8192SU_usb_linux_v2.6.0005.20091112/ modules V=1

clean:
	make  -C  $(shell pwd)/rtl8712_8188_8191_8192SU_usb_linux_v2.6.0005.20091112/ clean
	
install:
	make  -C  $(shell pwd)/rtl8712_8188_8191_8192SU_usb_linux_v2.6.0005.20091112/ install
