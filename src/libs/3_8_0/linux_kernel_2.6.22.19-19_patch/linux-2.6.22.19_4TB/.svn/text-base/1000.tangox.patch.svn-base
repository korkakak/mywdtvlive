+	if (!is_tango3_chip())
+		return(0);
+	else {
+		switch(chip_id) {
+			case 0x8652:
+				rev += 2;
+			case 0x8654:
+			case 0x8644:
+				return((rev == revid) ? 1 : 0);
+			default:
+				return(0);
+		}
+	}
+}
