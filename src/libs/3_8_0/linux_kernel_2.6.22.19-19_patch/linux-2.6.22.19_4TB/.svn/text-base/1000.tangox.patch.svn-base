+/*
+ * helpers to access cpu block registers
+ */
+#define RD_CPU_REG32(r)		\
+		gbus_read_reg32(REG_BASE_cpu_block + (r))
+
+#define WR_CPU_REG32(r, v)	\
+		do {								\
+			gbus_write_reg32(REG_BASE_cpu_block + (r), (v));	\
+			iob();							\
+		} while(0)	
